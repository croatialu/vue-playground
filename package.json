{
  "name": "vitesse-lite-bun",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.1.3",
  "scripts": {
    "build": "vite build",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.29.0",
    "@vueuse/core": "^10.9.0",
    "rxdb": "^15.17.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.13.3",
    "@iconify-json/carbon": "^1.1.31",
    "@types/node": "^20.12.7",
    "@unocss/eslint-config": "^0.59.1",
    "@unocss/eslint-plugin": "^0.59.1",
    "@unocss/reset": "^0.59.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue-macros/volar": "^0.18.18",
    "@vue/test-utils": "^2.4.5",
    "eslint": "^9.0.0",
    "eslint-plugin-format": "^0.1.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^8.15.6",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.3",
    "typescript": "^5.4.5",
    "unocss": "^0.59.1",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.9.1",
    "unplugin-vue-router": "^0.8.5",
    "vite": "^5.2.8",
    "vite-plugin-jsx": "^0.0.5",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.12"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
